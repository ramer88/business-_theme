@font-face {
			font-family: "Gotham Black Regular";
			src: url("../fonts/Gotham Black Regular/Gotham Black Regular.eot");
			src: url("../fonts/Gotham Black Regular/Gotham Black Regular.eot?#iefix")format("embedded-opentype"),
			url("../fonts/Gotham Black Regular/Gotham Black Regular.woff") format("woff"),
			url("../fonts/Gotham Black Regular/Gotham Black Regular.ttf") format("truetype");
			font-style: normal;
			font-weight: normal;
	}

